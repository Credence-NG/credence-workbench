---
import LayoutCommon from '../app/LayoutCommon.astro';
import { pathRoutes } from '../config/pathRoutes';
---

<LayoutCommon>
	<div class="flex flex-col min-h-screen bg-white dark:bg-gray-900 -z-10">
		<header>
			<nav
				class="bg-white dark:bg-gray-900 w-full z-20 top-0 left-0 border-gray-200 dark:border-gray-600"
			>
				<div
					class="w-full max-w-screen-xl mx-auto flex flex-wrap items-center p-4"
				>
					<button
						data-drawer-target="default-sidebar"
						data-drawer-toggle="default-sidebar"
						aria-controls="default-sidebar"
						type="button"
						class="inline-flex items-center p-2 w-10 h-10 justify-center text-sm text-gray-500 rounded-lg lg:hidden hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-gray-200 dark:text-gray-400 dark:hover:bg-gray-700 dark:focus:ring-gray-600"
						aria-expanded="false"
					>
						<span class="sr-only">Open main menu</span>
						<svg
							id="toggleMobileHamburger"
							class="w-5 h-5"
							aria-hidden="true"
							xmlns="http://www.w3.org/2000/svg"
							fill="none"
							viewBox="0 0 17 14"
						>
							<path
								stroke="currentColor"
								stroke-linecap="round"
								stroke-linejoin="round"
								stroke-width="2"
								d="M1 1h15M1 7h15M1 13h15"></path>
						</svg>

						<svg
							id="toggleMobileHamburgerClose"
							class="hidden w-6 h-6"
							fill="currentColor"
							viewBox="0 0 20 20"
							xmlns="http://www.w3.org/2000/svg"
						>
							<path
								fill-rule="evenodd"
								d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z"
								clip-rule="evenodd"></path>
						</svg>
					</button>
					<a href="/" class="flex items-center">
						<img
							src="/images/CREDEBL_ICON.png"
							class="h-8 mr-3"
							alt="Flowbite Logo"
						/>
						<span
							class="hidden md:block self-center text-2xl font-semibold whitespace-nowrap dark:text-white"
							>CREDEBL</span
						>
					</a>
					<div class="hidden lg:block ml-2">
						<a
							href="/"
							class="text-gray-600 py-2 bg-white hover:bg-gray-100 focus:ring-4 focus:ring-gray-200 font-medium rounded-lg text-md px-4 lg:px-3 lg:py-2.5 ml-4 dark:bg-primary-600 dark:hover:bg-primary-700 focus:outline-none dark:focus:ring-primary-800"
							>Pricing</a
						>
						<a
							href="https://docs.credebl.id"
							class="text-gray-600 py-2 bg-white hover:bg-gray-100 focus:ring-4 focus:ring-gray-200 font-medium rounded-lg text-md px-4 lg:px-3 lg:py-2.5 ml-4 dark:bg-primary-600 dark:hover:bg-primary-700 focus:outline-none dark:focus:ring-primary-800"
							>Documentation</a
						>
						<a
							href="/explore"
							class="text-gray-600 py-2 bg-white hover:bg-gray-100 focus:ring-4 focus:ring-gray-200 font-medium rounded-lg text-md px-4 lg:px-3 lg:py-2.5 ml-4 dark:bg-primary-600 dark:hover:bg-primary-700 focus:outline-none dark:focus:ring-primary-800"
							>Explore Organizations</a
						>
					</div>
					<div class="flex md:order-2 ml-auto">
						<a
							href="https://twitter.com/i/flow/login?redirect_after_login=%2Fcredebl"
							class="mr-3 mt-2 hidden md:block"
							target="_blank"
						>
							<svg
								xmlns="http://www.w3.org/2000/svg"
								width="20"
								height="18"
								viewBox="0 0 20 18"
								fill="none"
