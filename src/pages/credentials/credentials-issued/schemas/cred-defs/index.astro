---
import LayoutSidebar from '../../../../../app/LayoutSidebar.astro';
import CredDefSelection from '../../../../../components/Issuance/CredDefSelection';
import { pathRoutes } from '../../../../../config/pathRoutes';
import { checkUserSession } from '../../../../../utils/check-session';

const response = await checkUserSession(Astro.cookies);
const route: string = pathRoutes.auth.sinIn
if (!response) {
	return Astro.redirect(route);
}
---
<LayoutSidebar>
	<CredDefSelection client:load/>
</LayoutSidebar>
